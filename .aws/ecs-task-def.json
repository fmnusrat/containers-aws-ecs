{
    "family": "github-actions-task-def-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
        "name": "github-actions-container",
        "image": "602480057045.dkr.ecr.ap-south-1.amazonaws.com/my-ecr-repo",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/github-actions-tutorial-log-group",
                "awslogs-region": "us-south-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::992382657785:role/ecsTaskExecutionRoleNoorGithub",
    "taskRoleArn": "arn:aws:iam::992382657785:role/ecsTaskRoleNoorGitHub"
}